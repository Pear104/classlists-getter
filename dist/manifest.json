{
  "manifest_version": 3,
  "name": "FPTU Move Out Class Tool",
  "description": "Hỗ trợ sinh viên FPTU xem lịch, chuyển lớp nhanh",
  "version": "1.2",
  "content_scripts": [
    {
      "js": ["index.js"],
      "css": ["index.css"],
      "matches": ["https://fap.fpt.edu.vn/*"]
    }
  ],
  "action": {
    "default_icon": "image.png"
  },
  "icons": {
    "16": "image.png",
    "32": "image.png",
    "48": "image.png",
    "128": "image.png"
  }
}
